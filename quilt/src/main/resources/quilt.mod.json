{
  "schema_version": 1,
  "quilt_loader": {
    "group": "com.telepathicgrunt",
    "id": "fullstackwatchdog",
    "version": "${version}",
    "metadata": {
      "name": "FullStack Watchdog",
      "description": "Makes watchdog print the entire stacktrace of every thread because truncating stacktraces is stupid.",
      "contributors": {
        "TelepathicGrunt": "Owner"
      },
      "license": "LGPL-3.0-or-later",
      "icon": "assets/fullstackwatchdog/fullstackwatchdog_logo.png",
      "contact": {
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/fullstack-watchdog",
        "issues": "https://github.com/TelepathicGrunt/FullStackWatchdog/issues"
      }
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "com.telepathicgrunt.repurposedstructures.quilt.FullStackWatchdogQuilt",
      "modmenu": "com.telepathicgrunt.repurposedstructures.configs.fabricbase.ModMenuCompat"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": "*"
      },
      {
        "id": "quilted_fabric_api",
        "versions": "*"
      },
      {
        "id": "minecraft",
        "versions": ">=1.19.2"
      }
    ]
  },
  "mixin": [
    "fullstackwatchdog-quilt.mixins.json",
    "fullstackwatchdog-fabric-base.mixins.json",
    "fullstackwatchdog-common.mixins.json"
  ]
}
